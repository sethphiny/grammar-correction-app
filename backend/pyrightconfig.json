{
    "venvPath": ".",
    "venv": "venv",
    "pythonVersion": "3.12",
    "pythonPlatform": "Darwin",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.12",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                ".",
                "./services",
                "./models"
            ]
        }
    ],
    "include": [
        "**/*.py"
    ],
    "exclude": [
        "**/venv",
        "**/__pycache__",
        "**/node_modules"
    ],
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": "none",
    "typeCheckingMode": "basic"
}
